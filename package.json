{
    "name": "piolog-cli",
    "version": "1.1.0-beta",
    "description": "Command-line interface for piolog",
    "main": "src/js/piolog-cli.js",
    "scripts": {
        "main": "LOG4JS_CONFIG=log4js-config.json node src/js/piolog-cli.js",
        "debug": "LOG4JS_CONFIG=log4js-config.json node-debug src/js/piolog-cli.js",
        "test": "LOG4JS_CONFIG=log4js-config.json istanbul test _mocha -- test/js -R spec --recursive",
        "cover": "LOG4JS_CONFIG=log4js-config.json istanbul cover _mocha -- test/js -R spec --recursive"
    },
    "dependencies": {
        "commander": "~2.9.0",
        "handlebars": "~4.0.4",
        "log4js": "~0.6.27",
        "js-beautify": "~1.5.10",
        "piolog": "~1.1.0-beta"
    },
    "devDependencies": {
        "del": "~2.0.2",
        "gulp": "~3.9.0",
        "gulp-concat": "~2.6.0",
        "gulp-sourcemaps": "~1.6.0",
        "gulp-uglify": "~1.4.2",
        "istanbul": "~0.4.0",
        "mocha": "~2.3.3",
        "unit.js": "~2.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cicithesquirrel/piolog-cli.git"
    },
    "author": "Cyril \"cicithesquirrel\"",
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/cicithesquirrel/piolog-cli/issues"
    },
    "homepage": "https://github.com/cicithesquirrel/piolog-cli#readme"
}
